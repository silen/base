<extend name="Public/base"/>


<block name="nav">
    <div class="am-cf am-padding">
        <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">参与人数</strong> /
            <small>统计</small>
        </div>
    </div>

</block>
<block name="body">
    <div class="am-g">

    </div>

    <div class="am-g">
        <div class="am-u-sm-12">
            <form class="am-form">
                <table style="border: 1px solid #e9e9e9;" class="am-table am-table-striped am-table-hover table-main">
                    <thead>
                    <tr>
                        <th class="table-author">日期</th>
                        <th class="table-title">人数</th>
                    </tr>
                    </thead>
                    <tbody>
                    <volist name="list" id="data">
                        <tr>
                            <td>{:date('Y年m月d日', strtotime($data['time']))}</td>
                            <td>{$data.c}</td>
                        </tr>

                    </volist>
                    </tbody>
                </table>
                <div class="am-cf tk-pagenav">
                    {$show}
                </div>

            </form>
        </div>

    </div>
</block>
